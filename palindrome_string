#include<iostream>
using namespace std;
int main()
{
    int n,flag=0;
    cout<<"Length of word";
    cin>>n;
    char a[n+1];
    cin>>a;
    for(int i=0;i<n;i++)
    {
        if(a[i]!=a[n-1-i])
        {
            cout<<a[i];
            flag =1;
            break;
        }
    }
    if(flag==0)
    {
        cout<<"palindrome";
        
    }
    else
    cout<<"Not a palindrome";
    return 0;
}
